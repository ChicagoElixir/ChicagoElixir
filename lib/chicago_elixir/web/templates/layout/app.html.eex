      <!-- TESTIMONIAL -->
      <section id="testimonial">
        <div class="container-fluid">
          <div class="row">

            <div class="col-lg-6 col-lg-offset-6 col-md-6 col-md-offset-6 no-padding">
              <div class="testimonial-inner">
                <h2 class="hidden">testimonial</h2>

                <div id="testimonial-carousel">
                  <div class="item">
                    <img class="img-circle" src="img/pedro.jpg" alt="">
                    <p class="lead">Very happy to see the meetup growing in attendance, it shows Elixir is getting traction in Chicago.</p>
                    <p class="name">Pedro Assumpcao</p>
                  </div>

                  <div class="item">
                    <img class="img-circle" src="img/michael-king.jpg" alt="">
                    <p class="lead">Great, learning a lot every time I can make it.</p>
                    <p class="name">Michael King</p>
                  </div>

                  <div class="item">
                    <img class="img-circle" src="img/greg.jpg" alt="">
                    <p class="lead">First time there, very pleasant and worthwhile</p>
                    <p class="name">Greg Baran</p>
                  </div>
                </div>
              </div>
            </div>

          </div>
        </div>
      </section>

      <!-- SPONSORS -->
      <section id="sponsors">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <h2 class="uppercase">sponsors</h2>
              <p class="lead">Our sponsors help the community thrive</p>

              <div class="sponsor">
                <img class="img-responsive" src="img/hashrocket-logo.png" alt="hashrocket logo">
                Hashrocket - A full-stack development and design consultancy specializing in Elixir, Phoenix and Ruby on Rails. Based in Chicago, IL and Jacksonville, FL.
                Hashrocket provides the meetup with space to meet, food, drinks, and swag.
                <a href="https://hashrocket.com">Visit Hashrocket</a>
              </div>

              <div class="sponsor">
                <img class="img-responsive" src="img/oreilly-logo.png" alt="oreilly logo">
                O'REILLY - We partnered with O'Reilly, one of the most well known publishers in the tech community to provide discounts and resources to our meetup attendees.
                <a href="https://www.oreilly.com/">Visit O'REILLY</a>
              </div>

            </div>
          </div>
        </div>
      </section>

      <!-- google map -->
      <div id="gmap_canvas"></div>

